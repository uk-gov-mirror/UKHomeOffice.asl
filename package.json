{
  "name": "asl",
  "version": "1.1.0",
  "description": "Animal Science Licensing",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon -r dotenv/config -e js,jsx,json -w .",
    "test": "npm run test:lint",
    "test:lint": "eslint .",
    "test:functional": "funkie --driver chromedriver npm run test:functional:ci",
    "test:functional:ci": "asl-test ./tests/functional/config",
    "build": "npm run build:js && npm run build:css",
    "build:js": "webpack",
    "prebuild:css": "mkdir -p ./public/css",
    "build:css": "npm-sass ./assets/sass/app.scss > ./public/css/app.css",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UKHomeOffice/asl.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UKHomeOffice/asl/issues"
  },
  "homepage": "https://github.com/UKHomeOffice/asl#readme",
  "dependencies": {
    "@asl/components": "^1.2.0",
    "@asl/pages": "^14.2.1",
    "@asl/service": "^6.5.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "@ukhomeoffice/asl-functional-testing": "^1.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "dotenv": "^5.0.1",
    "eslint": "^4.15.0",
    "eslint-config-lennym": "^2.0.1",
    "eslint-plugin-undocumented-env": "^1.0.0",
    "funkie": "0.0.6",
    "funkie-chromedriver": "0.0.1",
    "nodemon": "^1.17.4",
    "npm-sass": "^2.3.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.7"
  }
}
